@{
    PageData["title"] = "GuessIt Soccer | League";
}
<body ng-controller="SingleLeagueCtrl">
    <table class="table-striped table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Team Name</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="team in teamsFilter">
                <td>{{team.teamID}}</td>
                <td>{{team.name}}</td>
            </tr>
        </tbody>
    </table>
</body>
